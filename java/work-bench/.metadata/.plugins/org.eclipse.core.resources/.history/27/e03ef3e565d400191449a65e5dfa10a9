package com.dev.threadss;

public class Thread4 extends Thread{

System.out.println("SyncMain Thread strated...");

Printer p;
public Thread4 (Printer pref){
	p=pref;
	
}
new Thread(p).start();

@Override
	public void run() {
		for (int i = 0; i <=10; i++) {
			System.out.println("j= "+i);
		}

Printer2 p2;


System.out.println("SyncMain Thread terminated...");
}
}
}
class AllOperators 
{
	public static void main(String[] args) 
	{
	 int a=10;
	 int b=20;
	 int c=a*b;
	 System.out.println("a*b"+ c);//Arithmetic
int d=a+=b;
	 System.out.println("a+=b"+ d);//assignment
	a++;
	 System.out.println("a++"+ a);//increment
	 boolean e=a!=b;
	 System.out.println("a!=b"+e );//relational
	 boolean b1=true;
	 boolean f=!b1;
	 System.out.println("!b"+ f);//Logical
int g=a&b;
 	 System.out.println("a&b"+ g);//Bitwise


		
	}
}

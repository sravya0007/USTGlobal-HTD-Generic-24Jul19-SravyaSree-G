class AssignmentOperator 
{
	public static void main(String[] args) 
	{
		int i,j;
		i=100;
		j=20;
		System.out.println("i= "+i);
	
		System.out.println("j= "+j);
		i+=j;
		System.out.println("i= "+i);
		i-=j;
		System.out.println("i= "+i);
		i*=j;
		System.out.println("i= "+i);
		i%=j;
		System.out.println("i= "+i);
		i/=j;
		System.out.println("i= "+i);
		


	}
}

package com.dev.lambdaa;

public class test {

	public static void main(String[] args) {
		/*
		 * // TODO Auto-generated method stub FuncInt f=() -> { for(int i=0;i<10;i++) {
		 * 
		 * System.out.println(i+1);
		 * 
		 * } };
		 */
		
		FuncInt f=()->{
		try {
			int i=10/0;
			System.out.println(i);
			
		}
		catch(Exception e){
			System.out.println("Exception.."+e);
		}
		};
		FucnInt2 f2=(int i) -> { for(int j=1;j<=i;j++) { System.out.println(j);
		}
		
		};
		
		f.printVal();
		f2.printVal(5);
		}

}

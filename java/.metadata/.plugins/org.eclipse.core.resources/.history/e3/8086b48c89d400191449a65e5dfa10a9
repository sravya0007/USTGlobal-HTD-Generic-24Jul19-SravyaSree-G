public class empData {
	
	
	

		public static void main(String[] args) {
			emp d=new emp();
			emp d1=new emp();
			emp d2=new emp();
			emp d3=new emp();
			d.setId("2");
			d.setName("cesor");
			d.setId("brown");
			d.setPassword("greman shepard");
			d1.setId("3");
			d1.setName("preethi");
			d1.setEmail("white");
			d1.setPassword("Dalmation");
			d2.setId("4");
			d2.setName("snupy");
			d2.setEmail("black");
			d2.setPassword("Doberman");
			d3.setId("5");
			d3.setName("pink");
			d3.setEmail("white");
			d3.setPassword("poddle");
			 
			emp []emp= {d,d1,d2,d3};
			for(int i=0;i<emp.length;i++) {
				System.out.println("name: "+emp[i].getName());
				System.out.println("Id: "+emp[i].getId());
				System.out.println("Email: "+emp[i].getEmail());
				System.out.println("Password: "+emp[i].getPassword());
				System.out.println("______________________________________");
			}
		}

